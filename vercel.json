{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/prerender.js": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate, max-age=0"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/sitemap.xml", "destination": "/api/sitemap" },
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/src/(.*)", "destination": "/src/$1" },
    { "source": "/", "destination": "/api/prerender?path=/" },
    { "source": "/index.html", "destination": "/api/prerender?path=/" },
    { "source": "/intro", "destination": "/api/prerender?path=/intro" },
    { "source": "/q/Top-Explore-Questions/general/:id", "destination": "/api/prerender?path=/q/Top-Explore-Questions/general/:id" },
    { "source": "/q/Top-Explore-Questions/roth-conversions/:id", "destination": "/api/prerender?path=/q/Top-Explore-Questions/roth-conversions/:id" },
    { "source": "/q/Top-Explore-Questions/financial-planning/:id", "destination": "/api/prerender?path=/q/Top-Explore-Questions/financial-planning/:id" },
    { "source": "/q/Top-Explore-Questions/medicare/:id", "destination": "/api/prerender?path=/q/Top-Explore-Questions/medicare/:id" },
    { "source": "/q/Top-Explore-Questions/persona/:id", "destination": "/api/prerender?path=/q/Top-Explore-Questions/persona/:id" },
    { "source": "/((?!api|assets|src).*)", "destination": "/api/prerender?path=/$1" }
  ]
}